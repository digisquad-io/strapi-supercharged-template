{
  "name": "strapi-supercharged-template",
  "private": true,
  "version": "0.3.3",
  "description": "A Strapi supercharged application",
  "author": {
    "name": "digisquad.io",
    "url": "https://digisquad.io",
    "email": "contact@digisquad.io"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:digisquad-io/strapi-supercharged-template.git"
  },
  "types": "types",
  "files": [
    "types"
  ],
  "strapi": {
    "uuid": "35d59bef-2343-4906-9e63-1b78f95028cb"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.d.ts": [
      "prettier --write"
    ]
  },
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "config:dump": "strapi configuration:dump -f ./.tmp/fields.json -p",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write .",
    "test": "run-p test:*",
    "test:tsc": "tsc",
    "test:eslint": "eslint .",
    "test:prettier": "prettier --check .",
    "test:e2e": "echo \"[warning] no e2e test implemented yet\"",
    "test:unit": "echo \"[warning] no unit test implemented yet\"",
    "commit": "commit",
    "release": "run-s test release:*",
    "release:standard-version": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@commitlint/prompt-cli": "13.2.1",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "babel-eslint": "10.1.0",
    "commitlint": "13.2.1",
    "eslint": "8.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-redux-saga": "1.2.1",
    "lint-staged": "11.2.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "sqlite3": "5.0.2",
    "standard-version": "9.3.2",
    "strapi-supercharged": "0.4.0",
    "typescript": "4.4.4",
    "yorkie": "2.0.0"
  },
  "dependencies": {
    "knex": "0.95.11",
    "koa-joi-router": "8.0.0",
    "pg": "8.7.1",
    "strapi": "3.6.8",
    "strapi-admin": "3.6.8",
    "strapi-connector-bookshelf": "3.6.8",
    "strapi-plugin-content-manager": "3.6.8",
    "strapi-plugin-content-type-builder": "3.6.8",
    "strapi-plugin-email": "3.6.8",
    "strapi-plugin-i18n": "3.6.8",
    "strapi-plugin-upload": "3.6.8",
    "strapi-plugin-users-permissions": "3.6.8",
    "strapi-utils": "3.6.8"
  },
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": "6.0.0"
  },
  "license": "MIT"
}
